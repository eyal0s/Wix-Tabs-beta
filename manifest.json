{
  "name": "Wix Tabs - get to know Wix users",
  "version": "0.1",
  "background": {
    "scripts": ["application.js"],
    "persistent": false
  },

  "chrome_url_overrides": {
    "newtab": "wixtabs.html"
  },

  "manifest_version": 2,

  "icons": { "16": "wixtabs16.png",
           "48": "wixtabs48.png",
          "128": "wixtabs128.png" },

  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "update_url": "https://dl.dropboxusercontent.com/u/54065586/updates.xml",


  "permissions": [
    "tabs",
    "https://dl.dropboxusercontent.com/u/54065586/"
  ]
}
